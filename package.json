{
  "name": "vpcvdc.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vpcvdc.github.io",
  "dependencies": {
    "@headlessui/react": "0.2.0",
    "fbt": "0.16.1",
    "history": "5.0.0",
    "react": "0.0.0-experimental-3310209d0",
    "react-dom": "0.0.0-experimental-3310209d0",
    "react-router-dom": "0.0.0-experimental-ffd8c7d0",
    "web-vitals": "1.0.1",
    "workbox-background-sync": "6.0.2",
    "workbox-broadcast-update": "6.0.2",
    "workbox-cacheable-response": "6.0.2",
    "workbox-core": "6.0.2",
    "workbox-expiration": "6.0.2",
    "workbox-google-analytics": "6.0.2",
    "workbox-navigation-preload": "6.0.2",
    "workbox-precaching": "6.0.2",
    "workbox-range-requests": "6.0.2",
    "workbox-routing": "6.0.2",
    "workbox-strategies": "6.0.2",
    "workbox-streams": "6.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/node": "7.12.10",
    "@craco/craco": "6.0.0",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/postcss7-compat": "2.0.2",
    "@tailwindcss/typography": "0.3.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "4.0.1",
    "@testing-library/user-event": "12.6.0",
    "autoprefixer": "9.8.6",
    "babel-plugin-fbt": "0.17.0",
    "babel-plugin-fbt-runtime": "0.9.13",
    "gh-pages": "3.1.0",
    "postcss": "7.0.35",
    "react-scripts": "4.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.0.2"
  },
  "scripts": {
    "clean": "rimraf node_modules/.cache build .enum_manifest.json .src_manifest.json .source_strings.json src/locales",
    "prefbt": "fbt-manifest --src src",
    "fbt": "fbt-collect --pretty --manifest < .src_manifest.json > .source_strings.json",
    "postfbt": "fbt-translate --translations translations/*.json --jenkins --output-dir src/locales",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "craco start",
    "prebuild": "npm run clean",
    "build": "npm run fbt",
    "postbuild": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
